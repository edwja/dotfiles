"*":
  "atom-beautify":
    ruby: {}
  "clear-night-ui":
    layoutMode: "Compact"
  core:
    allowPendingPaneItems: false
    closeDeletedFileTabs: true
    disabledPackages: [
      "autocomplete-plus"
      "wrap-guide"
      "guard-rspec"
      "linter-erb"
      "rubocop-auto-correct"
    ]
    telemetryConsent: "no"
    themes: [
      "one-light-ui"
      "one-light-syntax"
    ]
  editor:
    fontFamily: "Monaco"
    fontSize: 13
    showIndentGuide: true
    softWrapAtPreferredLineLength: true
  "exception-reporting":
    userId: "3fbf472d-932d-494b-acfa-94e6b329365f"
  "git-blame":
    columnWidth: 339
  github:
    gitDiagnostics: true
  "guard-rspec":
    command: "bundle exec guard"
  linter:
    lintOnOpen: false
  "linter-rubocop":
    command: "/Users/andyedwards/.rbenv/shims/rubocop -R"
  "linter-ui-default":
    panelHeight: 174
  "rubocop-auto-correct":
    autoRun: true
    correctFile: true
    debugMode: true
    rubocopCommandPath: "/Users/andyedwards/.rbenv/shims/rubocop"
  "ruby-test":
    cucumberAllCommand: "bundle exec cucumber --color features"
    cucumberFileCommand: "bundle exec cucumber --color {relative_path}"
    cucumberSingleCommand: "bundle exec cucumber --color {relative_path}:{line_number}"
    minitestAllCommand: "bundle exec ruby -I test test"
    minitestFileCommand: "bundle exec ruby -I test {relative_path}"
    minitestSingleCommand: "bundle exec ruby -I test {relative_path} -n \"/{regex}/\""
    rspecAllCommand: "spring rspec --tty spec"
    rspecFileCommand: "spring rspec --tty {relative_path}"
    rspecSingleCommand: "spring rspec --tty {relative_path}:{line_number}"
    specFramework: "rspec"
  welcome:
    showOnStartup: false
".ruby.source":
  editor:
    tabType: "soft"
